OUTPUT_FORMAT("elf64-ia64-little")
ENTRY(start)

SECTIONS {
	.boot 0x4400000: AT (0x4400000) {
		loader_start = .;
		*(BOOTSTRAP);
		*(.text);
		*(.text.*);
		*(.sdata);
		*(.sdata2);
		*(.sbss);
		*(.rodata);
		*(.rodata.*);
		*(.data .data.*);       /* initialized data */
		__gp = . ;
		*(.got .got.*);
		*(.bss);                /* uninitialized static variables */
		*(COMMON);
		loader_end = .;
		payload_start = .;
		*(.payload);
		payload_end = .;
	}

	/DISCARD/ : {
		*(.*);
	}
}
