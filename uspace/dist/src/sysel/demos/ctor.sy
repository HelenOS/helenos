class ConstructorDemo is
	fun Main(), static is
		var a : A;

		a = new A(1);
		Console.Write("a.v = ");
		Console.WriteLine(a.v);
	end
end

class A is
	var v : int;

	new(i : int) is
		Console.WriteLine("A.new()");
		v = i;
	end
end
