-- Using the List class from the library.
class ListDemo is
	fun Main(), static is
		var list : List/int;

		list = new List/int();

		list.Append(5);
		list.Append(6);
		list.Append(7);
		list.Append(8);

		var e : IEnumerator/int;

		e = list.GetEnumerator();
		while e.MoveNext() do
			Console.WriteLine(e.Data);
		end
	end
end
