class ArrayDemo is
	fun Main(), static is
		var a : int[,];
		var i : int;

		a = new int[3, 2];

		a[0, 0] = 1;
		a[1, 0] = 2;
		a[2, 0] = 3;

		i = 0;
		while i < 3 do
			Console.WriteLine(a[i, 0]);
			i = i + 1;
		end
	end
end
