-- Using the Map class from the library.
class MapDemo is
	fun Main(), static is
		var map : Map/string/int;

		map = new Map/string/int();

		map["one"] = 1;
		map["two"] = 2;
		map["three"] = 3;
		map["four"] = 4;

		var e : IEnumerator/string;

		e = map.GetEnumerator();
		while e.MoveNext() do
			Console.Write(e.Data);
			Console.Write(" -> ");
			Console.WriteLine(map[e.Data]);
		end
	end
end
