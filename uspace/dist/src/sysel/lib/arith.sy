class Arith is
	-- Return factorial of n.
	fun Factorial(n : int) : int, static is
		var i : int;
		var val : int;

		i = 1;
		val = 1;

		while i <= n do
			val = val * i;
			i = i + 1;
		end

		return val;
	end
end
